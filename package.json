{
  "name": "bootstrap",
  "version": "1.0.0",
  "description": "Bootstrapping a vite web app",
  "main": "www/index.html",
  "scripts": {
    "build": "vite build",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "test:storybook": "npx test-storybook",
    "test:storybook:ci": "concurrently -k \"npm:storybook\" \"wait-on tcp:6006 && npx test-storybook\"",
    "start": "vite dev",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "npm-run-all lint:prettier lint:eslint lint:stylelint",
    "lint:fix": "npm-run-all lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:eslint": "eslint \"**/*.tsx\" \"**/*.ts\"",
    "lint:eslint:fix": "eslint \"**/*.tsx\" \"**/*.ts\" --fix",
    "lint:prettier": "prettier --config .prettierrc.json --check **/*.tsx **/*.ts",
    "lint:prettier:fix": "prettier --config .prettierrc.json --write **/*.tsx **/*.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint:stylelint:fix": "stylelint **/*.scss --fix"
  },
  "author": "Greg Skinner",
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.2",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "ts-node": "^10.9.1",
    "vite": "^4.4.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-onboarding": "^1.0.7",
    "@storybook/blocks": "^7.1.0",
    "@storybook/jest": "^0.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "sass": "^1.63.6",
    "storybook": "^7.1.0",
    "stylelint": "14.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "wait-on": "^7.0.1"
  }
}
